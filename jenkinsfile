node {
    def app
    stage('Clone repository') {
        checkout scm
    }

    stage('Build') {
       app = docker.build("zip_job/Dockerfile")
    }

    stage('Run script') {
  
        app.inside {
            sh 'cd /tmp && python3 /tmp/zip_job.py'
        }
    }
    
    stage('Upload') {
  
        app.inside {
            echo 'Hello'
        }
    }
    
    stage('Push image') {
        echo "Helloooo Push"
    }
}
